<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Entities\RouteServerFilter" table="route_server_filters" tableName="route_server_filters" repository-class="Repositories\RouteServerFilter">
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="prefix" type="string" length="43" nullable="true"/>
    <field name="protocol" type="smallint" nullable="true"/>
    <field name="action_advertise" type="string" nullable="true"/>
    <field name="action_receive" type="string" nullable="true"/>
    <field name="enabled" type="boolean" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="order_by" type="integer" nullable="false"/>
    <field name="live" type="string" nullable="false"/>
    <unique-constraints>
      <unique-constraint name="cust_order" columns="customer_id,order_by"/>
    </unique-constraints>
    <many-to-one field="customer" target-entity="Entities\Customer" inversed-by="routeServerFilters">
      <join-columns>
        <join-column name="customer_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="peer" target-entity="Entities\Customer" inversed-by="peerRouteServerFilters">
      <join-columns>
        <join-column name="peer_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="vlan" target-entity="Entities\Vlan" inversed-by="routeServerFilters">
      <join-columns>
        <join-column name="vlan_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
